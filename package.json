{
  "name": "canvg",
  "description": "Javascript SVG parser and renderer on Canvas",
  "homepage": "https://github.com/gabelerner/canvg",
  "keywords": [
    "javascript",
    "client",
    "browser",
    "svg",
    "canvas"
  ],
  "author": "Gabe Lerner <gabelerner@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/gabelerner/canvg.git"
  },
  "main": "canvg.node.js",
  "browser": "canvg.min.js",
  "version": "1.5.1",
  "scripts": {
    "start": "node test/browser.test.js",
    "build": "npm run build:browser && npm run build:node",
    "build:browser": "rollup -c",
    "build:node": "IS_NODE=1 rollup -c",
    "test": "ava --tap | tap-diff",
    "test:browser": "ava test/browser.test.js --tap | tap-diff",
    "test:node": "ava test/node.test.js --tap | tap-diff",
    "minify": "uglifyjs --mangle --compress if_return=true -o canvg.min.js canvg.js && uglifyjs --mangle --compress if_return=true -o canvg.node.min.js canvg.node.js"
  },
  "license": "MIT",
  "files": [
    "canvg.js",
    "canvg.min.js",
    "canvg.node.js",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "jsdom": "^8.1.0",
    "rgbcolor": "^1.0.1",
    "stackblur": "^1.0.0",
    "xmldom": "^0.1.22"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.6",
    "ava-is-running": "^1.0.3",
    "blink-diff": "^1.0.13",
    "bluebird": "^3.5.1",
    "canvas": "^1.4.0",
    "data-uri-to-buffer": "^2.0.0",
    "debug": "^2.6.9",
    "mkdirp": "^0.5.1",
    "pngjs-image": "^0.11.7",
    "puppeteer": "^1.6.1",
    "rollup": "^0.63.5",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "serve-handler": "^3.6.1",
    "tap-diff": "^0.1.1",
    "uglify-js": "^3.3.16"
  },
  "jspm": {
    "registry": "npm"
  }
}